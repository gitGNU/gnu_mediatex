Grammar:
@example
YEAR    [[:digit:]]@{4@}
MONTH   [[:digit:]]@{2@}
DAY     [[:digit:]]@{2@}
HOUR    [[:digit:]]@{2@}
MIN     [[:digit:]]@{2@}
SEC     [[:digit:]]@{2@}
DATE    @{YEAR@}-@{MONTH@}-@{DAY@},@{HOUR@}:@{MIN@}:@{SEC@}

HASH    [[:xdigit:]]@{32@}      
STATUS  [^[:blank:]\r\n]@{1,10@} 
NAME    [^[:blank:]\r\n]@{1,64@} 

file: lines

lines: lines line
     | line

line: DATE DATE DATE HASH HASH [[:digit:]]+ STATUS NAME
@end example

